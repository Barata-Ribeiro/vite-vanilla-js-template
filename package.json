{
  "name": "vite-vanilla-js",
  "description": "ViteJS starter template for vanilla JavaScript with some basic dependencies.",
  "version": "0.0.0",
  "type": "module",
  "keywords": [
    "vanilla",
    "javascript",
    "template",
    "boilerplate",
    "boilerplate-template",
    "postcss",
    "eslint",
    "prettier",
    "scaffold",
    "scaffold-template",
    "vite",
    "vitejs",
    "vite-template",
    "vitejs-template"
  ],
  "scripts": {
    "dev": "vite",
    "lint": "eslint --ext .js ./",
    "lint:fix": "eslint --fix --ext .js ./",
    "format": "prettier --write \"./**/*.{js,jsx,json,css}\"",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "vite build && vite preview"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "postcss": "^8.4.26",
    "prettier": "^3.0.0",
    "vite": "4.4.3",
    "vite-plugin-eslint": "^1.8.1"
  },
  "dependencies": {
    "the-new-css-reset": "^1.9.0"
  }
}
